<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/seguimient_al"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#BED9BA"
    android:backgroundTint="#BED9BA"
    tools:context=".control_salud">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#267331"
        android:gravity="center_horizontal"
        android:text=""
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="35dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#267331"
        android:gravity="center_horizontal"
        android:text=""
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="35dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/recomenaciones"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:text="Control de salud"
        android:fontFamily="@font/anek_devanagari_medium"
        android:textColor="@color/black"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:layout_width="345dp"
        android:layout_height="421dp"
        android:background="@drawable/corner"
        android:orientation="vertical"
        android:layout_marginTop="-250dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.582"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.529">

        <TableLayout
            android:id="@+id/info"
            android:layout_width="348dp"
            android:layout_height="415dp"
            android:background="@drawable/corner"
            android:stretchColumns="1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.507"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.348">

            <TableRow android:layout_marginTop="20dp">

                <TextView
                    android:layout_marginRight="20dp"
                    android:fontFamily="@font/anek_devanagari_medium"
                    android:padding="8dp"
                    android:text="Identificador Médico:"
                    android:textColor="@color/black" />

                <EditText
                    android:id="@+id/edtNMedico"
                    android:layout_marginRight="10dp"
                    android:background="@color/white"
                    android:inputType="none"
                    android:textColor="@color/black" />
            </TableRow>

            <TableRow android:layout_marginTop="10dp">

                <TextView
                    android:layout_marginRight="20dp"
                    android:fontFamily="@font/anek_devanagari_medium"
                    android:padding="8dp"
                    android:text="Comidas cronometradas:"
                    android:textColor="@color/black" />

                <EditText
                    android:id="@+id/edtcomidas"
                    android:layout_marginRight="10dp"
                    android:background="@color/white"
                    android:inputType="number"
                    android:textColor="@color/black" />
            </TableRow>

            <TableRow android:layout_marginTop="10dp">

                <TextView
                    android:layout_marginRight="20dp"
                    android:fontFamily="@font/anek_devanagari_medium"
                    android:padding="8dp"
                    android:text="Enfermedades"
                    android:textColor="@color/black" />

                <EditText
                    android:id="@+id/edtEnfer"
                    android:layout_marginRight="10dp"
                    android:background="@color/white"
                    android:inputType="text"
                    android:textColor="@color/black" />
            </TableRow>

            <TableRow android:layout_marginTop="10dp">

                <TextView
                    android:layout_marginRight="20dp"
                    android:fontFamily="@font/anek_devanagari_medium"
                    android:padding="8dp"
                    android:text="Calorías Consumidas:"
                    android:textColor="@color/black" />

                <EditText
                    android:id="@+id/edtTotalC"
                    android:layout_marginRight="10dp"
                    android:background="@color/white"
                    android:inputType="number"
                    android:textColor="@color/black" />
            </TableRow>

            <TableRow android:layout_marginTop="10dp">

                <TextView
                    android:layout_marginRight="20dp"
                    android:fontFamily="@font/anek_devanagari_medium"
                    android:padding="8dp"
                    android:text="Medicamentos"
                    android:textColor="@color/black" />

                <EditText
                    android:id="@+id/edtmedicina"
                    android:layout_marginRight="10dp"
                    android:background="@color/white"
                    android:inputType="text"
                    android:textColor="@color/black" />
            </TableRow>

            <TableRow android:layout_marginTop="10dp">

                <TextView
                    android:layout_marginRight="20dp"
                    android:fontFamily="@font/anek_devanagari_medium"
                    android:padding="8dp"
                    android:text="Telefono"
                    android:textColor="@color/black" />

                <EditText
                    android:id="@+id/edtNumber"
                    android:layout_marginRight="10dp"
                    android:background="@color/white"
                    android:inputType="number"
                    android:textColor="@color/black" />
            </TableRow>

            <TableRow android:layout_marginTop="10dp">

                <TextView
                    android:layout_marginRight="20dp"
                    android:fontFamily="@font/anek_devanagari_medium"
                    android:padding="8dp"
                    android:text="Nutriologo"
                    android:textColor="@color/black" />

                <EditText
                    android:id="@+id/edtnutri"
                    android:layout_marginRight="10dp"
                    android:background="@color/white"
                    android:inputType="text"
                    android:textColor="@color/black" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="10dp">

                <TextView
                    android:layout_marginRight="20dp"
                    android:fontFamily="@font/anek_devanagari_medium"
                    android:padding="8dp"
                    android:text=" Contacto Nutriólogo:"
                    android:textColor="@color/black" />

                <EditText
                    android:id="@+id/edtnumNutri"
                    android:layout_marginRight="10dp"
                    android:background="@color/white"
                    android:inputType="text"
                    android:textColor="@color/black" />
            </TableRow>
        </TableLayout>

    </LinearLayout>

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="185dp"
        android:layout_height="101dp"
        android:layout_marginBottom="768dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@mipmap/bann_nutri_background" />


    <Button
        android:id="@+id/button2"
        android:layout_width="323dp"
        android:layout_height="46dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="128dp"
        android:backgroundTint="#77A676"
        android:onClick="MenuP"
        android:text="Regresar "
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/button4"
        android:layout_width="323dp"
        android:layout_height="46dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="220dp"
        android:backgroundTint="#77A676"
        android:onClick="actualizar"
        android:text="Actualizar"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="80dp"
        android:layout_marginBottom="223dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/update" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="80dp"
        android:layout_marginBottom="132dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/comeback" />
</androidx.constraintlayout.widget.ConstraintLayout>